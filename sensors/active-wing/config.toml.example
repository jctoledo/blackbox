# Active Wing Configuration
# Copy this file to config.toml and update with your settings
# IMPORTANT: config.toml is git-ignored to protect your credentials

[wifi]
ssid = "YourWiFiNetwork"
password = "YourWiFiPassword"

[network]
mqtt_broker = "mqtt://192.168.1.100:1883"
tcp_server = "192.168.1.100:9000"

[telemetry]
interval_ms = 50  # 20 Hz

[calibration]
samples = 500
